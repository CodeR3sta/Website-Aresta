                <th>verified</th>
                <th>image verified</th>
                <th>selesai daftar</th>
                <th>Cek</th>
            </tr>

            <% row.forEach((element,nomor) => { %>
                <tr>
                    <td><%- nomor + 1 %> </td>
                <td><%- element.email %> </td>
                <td><%- element.phone %> </td>    
                    <td><%- element.namaUtama %> </td>
                    <td><%- element.sekolah %> </td>

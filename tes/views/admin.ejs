<body>
    <nav>
        <img src="/img/logoAresta1.png" alt="">
        <hr>
        <h1>ADMIN PAGE</h1>
        <hr>
        <h1 class="tablinks" onclick="openCity(event, 'search')" id="defaultOpen">search</h1>
        <h1 class="tablinks" onclick="openCity(event, 'status1')">Status_1</h1>
        <h1 class="tablinks" onclick="openCity(event, 'status2')">Status_2 (cek_foto)</h1>

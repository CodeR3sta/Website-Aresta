        <h1 class="tablinks" onclick="openCity(event, 'sertifikat')">e-sertifikat</h1>
        <h1 class="tablinks" onclick="openCity(event, 'excel')">Excel Download</h1>
        <hr>
        <form action="/code/resta/panitia/logout" method="post">
            <button id="logout">Logout</button>
        </form>
    </nav>


    <div class="tabcontent search" id="search">
        <div class="form">
            <form action="/code/resta/panitia/users" method="post">
                <input type="search" name="search" id="" placeholder="Nama Lengkap">
                <button type="submit">search</button>
            </form>
        </div>

        <table>
            <tr>
                <th>#</th>
                <th>Nama Ketua</th>
                <th>Asal Sekolah</th>
                <th>Lomba</th>
                <th>Tingkat</th>
                <th>Status</th>
                <th>Cek</th>
                <th>Delete</th>
            </tr>

            <% row.forEach((element,nomor) => { %>
                <tr>
                    <td><%- nomor + 1 %> </td>
                    <td><%- element.namaUtama %> </td>
                    <td><%- element.sekolah %> </td>
                    <td><%- element.lomba %> </td>
                    <td><%- element.tingkat %> </td>
                    <td><%- element.status %> </td>
                    <td><a href="/code/resta/panitia/users/views/<%= element.id %> ">cek</a></td>
                    <td><a href="/code/resta/panitia/users/delete/<%= element.id %> ">delete</a></td>
                </tr>
            <% }) %>

        </table>
    </div>
